<view class="transfer-freight">
  <view wx:for="{{liseview}}" wx:key="title" class="{{item.class}}" bindtap="{{item.bind}}">
    <view class="text">
      <view class="text1">{{item.title}}</view>
      <view class="text2">{{item.content}}</view>
    </view>
    <view class="dot">
      <image src="../../assets/images/arrowright.png"></image>
    </view>
    <view class="tf-img2">
      <image src="{{item.img}}"></image>
    </view>
  </view>
</view>